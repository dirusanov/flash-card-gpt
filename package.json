{
  "name": "chrome-extension-boilerplate-react",
  "version": "5.0.12",
  "description": "A chrome extension boilerplate built with React 18, Webpack 5, and Webpack Dev Server 4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lxieyang/chrome-extension-boilerplate-react.git"
  },
  "scripts": {
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:js": "node utils/webserver.js",
    "dev:css": "tailwindcss -w -i ./styles/tailwind.css -o src/assets/styles/tailwind.css",
    "build:css": "tailwindcss -m -i ./styles/tailwind.css -o src/assets/styles/tailwind.css --minify",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "watch:storybook": "start-storybook -p 6006",
    "storybook": "concurrently \"npm:watch:storybook\" \"npm:dev:css\"",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@types/katex": "^0.16.7",
    "@types/prismjs": "^1.26.5",
    "axios": "^1.7.7",
    "katex": "^0.16.22",
    "openai": "^4.72.0",
    "postcss-cli": "^10.1.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "reduxed-chrome-storage": "^3.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^8.4.4",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.4.4",
    "@storybook/testing-library": "^0.0.13",
    "@types/axios": "^0.9.36",
    "@types/chrome": "^0.0.202",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.15",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.29",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.8.3",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "type-fest": "^3.5.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "zip-webpack-plugin": "^4.0.1"
  }
}
